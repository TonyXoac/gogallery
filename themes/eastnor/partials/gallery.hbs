
<div class="row g-3">
    {{#each Images}}
    <div class="col-sm-12 col-md-6 col-lg-3">
        <picture>
            <a href="/photo/{{this.id}}">
                <img loading=lazy src="/img/{{this.id}}" width="100%" alt="{{this.name}}" class="img-fluid rounded">
            </a>
        </picture>
    </div>
    {{/each}}
</div>